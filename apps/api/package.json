{
	"name": "api",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"build": "tsup src/index.ts --format cjs",
		"clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
		"dev": "tsup src/index.ts --format cjs --watch --onSuccess \"node dist/index.js\"",
		"debug": "tsup src/index.ts --sourcemap --format cjs --watch --onSuccess \"node --inspect dist/index.js\"",
		"lint": "tsc --noEmit && eslint \"src/**/*.ts*\"",
		"start": "node dist/index.js"
	},
	"dependencies": {
		"bcryptjs": "^2.4.3",
		"chalk": "4.1.2",
		"cookie-parser": "^1.4.6",
		"cors": "^2.8.5",
		"csurf": "^1.11.0",
		"express": "^4.18.2",
		"express-rate-limit": "^6.7.0",
		"helmet": "^6.0.1",
		"hpp": "^0.2.3",
		"joi": "^17.9.1",
		"jsonwebtoken": "^9.0.0",
		"mongodb": "^5.1.0",
		"mongoose": "^7.0.3",
		"morgan": "^1.10.0",
		"ws": "^8.13.0"
	},
	"devDependencies": {
		"@faker-js/faker": "^7.6.0",
		"@types/bcryptjs": "^2.4.2",
		"@types/cookie-parser": "^1.4.3",
		"@types/cors": "^2.8.13",
		"@types/csurf": "^1.11.2",
		"@types/express": "^4.17.17",
		"@types/hpp": "^0.2.2",
		"@types/joi": "^17.2.3",
		"@types/jsonwebtoken": "^9.0.1",
		"@types/morgan": "^1.9.4",
		"@types/node": "^18.15.10",
		"@types/ws": "^8.5.4",
		"dotenv": "^16.0.3",
		"eslint": "^8.36.0",
		"eslint-config-custom-server": "workspace:*",
		"shared": "workspace:*",
		"tsconfig": "workspace:*",
		"tsup": "^6.7.0",
		"typescript": "^5.0.2"
	}
}
